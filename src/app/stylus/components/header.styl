@import '../variables.styl'

.header
  position fixed
  width 100%
  background rgba(33,47,51,0.8);
  box-sizing border-box
  padding: 30px 0
  height: 90px
  line-height 30px
  font-weight bold
  z-index 4
  top 0px
  
  &_logo
    line-height 46px
    font-size 38px
    margin-top -8px
    font-weight bold
    font-family "Helvetica Neue",Helvetica,Arial,sans-serif
    
  &_desc
    text-transform uppercase
  
    
